def findAllConcatenatedWordsInADict(words: list[str]) -> list[str]:
    word_set = set(words)
    memo = {}
    def dfs(word):
        if word in memo:
            return memo[word]
        for i in range(1, len(word)):
            prefix = word[:i]
            suffix = word[i:]
            if prefix in word_set and (suffix in word_set or dfs(suffix)):
                memo[word] = True
                return True
        memo[word] = False
        return False
    return [word for word in words if dfs(word)]
